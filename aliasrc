alias ..="cd ../"
alias ...="cd ../.."
alias ....="cd ../../.."
alias cat=bat
alias vim=nvim
alias dotf="cd $HOME/dotfiles"
alias ls="ls -G"
alias mkdir="mkdir -p"
alias nvimconfig="cd ~/dotfiles/config/nvim/"
alias g=git
alias ga="git add"
alias gb="git branch"
alias gco="git checkout"
alias gcb="git checkout -b"
alias gm="git checkout master"
alias gp="git pull"
alias grb="git rebase -i"
